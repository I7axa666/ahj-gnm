{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true,
      "jest": true      
    },
    "parser": "@babel/eslint-parser",
    "rules": {
      "no-plusplus": "off",
      "no-alert": "off",
      "no-tabs": 0,
      "no-mixed-operators": 0,
      "import/no-named-as-default": 0,
      "import/no-named-as-default-member": 0,
      "no-param-reassign": 0,
      "class-methods-use-this": 0
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    }
}